	<context:annotation-config />
	<context:component-scan base-package="com.harvey.w.core.test.bean"
	                        name-generator="com.harvey.w.core.spring.BeanNameGenerator" />
	<bean class="com.harvey.w.core.spring.ConfigProperiesPlaceHolder" />
	<bean class="com.harvey.w.core.test.bean.TestBeanA" />
	<bean id="beanBFactoryByXml" class="com.harvey.w.core.test.bean.TestBeanBFactory" />
